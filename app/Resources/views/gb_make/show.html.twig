{% extends 'base.html.twig' %}

{% block body %}
    <h1>Gb_make</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ gB_make.id }}</td>
            </tr>
            <tr>
                <th>Mname</th>
                <td>{{ gB_make.mName }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('gb_make_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('gb_make_edit', { 'id': gB_make.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
